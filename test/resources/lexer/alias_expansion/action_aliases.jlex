WHITESPACE      \s
DIGIT           [0-9]
NON_ZERO_DIGIT  [1-9]
NUMBER          0|{NON_ZERO_DIGIT}+{DIGIT}*
CHAR            [_a-zA-Z]
IDENTIFIER      {CHAR}({CHAR}|{DIGIT})*

%%

{IDENTIFIER}                        { return Id($$) }
{NUMBER}                            { return Number($$) }
"**HELLO**"{WHITESPACE}+"++WORLD++" { return Hello() }
.*                                  { return Error() }
"a"              "b"                { return AB() }

%%

