#= Definitions =#

%token END "end"
%token NUMBER

???INVALID_CHARS???

%type <int> expr
%type <int> NUMBER
%%
#= Productions =#
%start start
start -> expr END { println($1) }
expr -> expr '+' expr { $$ = $1 + $3 }
expr -> expr '-' expr { $$ = $1 - $3 }
expr -> expr '*' expr { $$ = $1 * $3 }
expr -> expr '/' expr { $$ = $1 / $3 }
expr -> '(' expr ')' { $$ = $3 }
expr -> NUMBER { $$ = $1 }

